{
  "name": "@mayacode/react-custom-hooks",
  "sideEffects": false,
  "version": "1.1.4",
  "description": "set of often used custom react hooks",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/index.js"
  ],
  "scripts": {
    "test": "jest --ci --coverage && codecov",
    "release-tests": "jest",
    "dev-tests": "jest --coverage --watch",
    "release": "np --test-script=release-tests",
    "lint": "eslint .",
    "format": "npm run prettier -- --write",
    "prettier": "prettier \"**/*.+(js|jsx|json)\"",
    "validate": "npm run lint && npm run prettier -- --list-different",
    "precommit": "lint-staged",
    "test-dev": "jest --coverage",
    "build": "webpack --mode=production",
    "prepublish": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mayacode/react-custom-hooks.git"
  },
  "keywords": [
    "react",
    "hooks"
  ],
  "author": "Maja Miarecki <maja.miarecki@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mayacode/react-custom-hooks/issues"
  },
  "homepage": "https://github.com/mayacode/react-custom-hooks#readme",
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@testing-library/react-hooks": "^3.7.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.8.1",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "source-map-loader": "^1.1.3",
    "ts-loader": "^8.0.12",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
